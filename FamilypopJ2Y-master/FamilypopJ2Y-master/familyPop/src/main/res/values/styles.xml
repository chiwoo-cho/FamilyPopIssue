<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="android:Theme.Light">
        <!--
            Theme customizations available in newer API levels can go in
            res/values-vXX/styles.xml, while customizations related to
            backward-compatibility can go here.
        -->
    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <color name = "color_white">#ffffff</color>

    <color name = "default_bg">#e6e6e6</color>
    <color name = "color_default_background">#1b1b1b</color>
    <color name = "color_default_string">#ffffff</color>
    <color name = "color_default_string1">#00A8C0</color>
    <color name = "color_default_string2">#969696</color>
    <color name = "color_default_string3">#F04E65</color>


    <color name = "color_button_state_normal">#1b1b1b</color>
    <color name = "color_button_state_onclick">#969696</color>
    <color name = "color_button_state_onclick2">#F04E65</color>


    <color name="red">#FA5858</color>
    <color name="green">#74DF00</color>
    <color name="blue">#58ACFA</color>
    <color name="yellow">#D7DF01</color>
    <color name="grey">#6E6E6E</color>
    <color name="purple">#B200FF</color>
    <color name="dncolor">#84FFE2</color>


    <style name = "testDialog" parent="android:Theme.Light">
        <item name="android:windowNoTitle">true</item>
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

    <style name = "Theme.Dialog" parent="android:Theme.Dialog">
        <item name="android:windowBackground">@android:color/transparent</item>
    </style>

</resources>
